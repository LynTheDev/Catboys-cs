<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>Catboys</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>CatboysCS</PackageId>
    <Title>CatboysCS</Title>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>omoNattie</Authors>
    <Description>A C# package that uses the catboys API.</Description>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/omoNattie/catboys-cs</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
